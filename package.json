{
  "name": "dynamodb-std",
  "description": "DynamoDB Single Table Design Implementation (aws-sdk-v3 compatible)",
  "version": "0.8.6",
  "private": false,
  "license": "MIT",
  "author": "Denis Zatsepin <denis@zatsepin.me>",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -d",
    "test": "jest",
    "lint": "eslint --cache --fix",
    "prepublish": "npm run lint && npm run build"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gameobsessed/dynamodb-std.git"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "singletabledesign",
    "std",
    "aws-sdk-v3"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.53.0",
    "@aws-sdk/lib-dynamodb": "3.53.0",
    "@aws-sdk/util-dynamodb": "3.53.0",
    "@aws/dynamodb-expressions": "0.7.3",
    "source-map-support": "^0.5.21",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@commitlint/top-level": "16.2.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "aws-sdk": "2.1084.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "5.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.6.0",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,html,json}": "yarn prettier --write",
    "src/**/*.{ts}": "yarn eslint --cache --fix"
  }
}
